# Frontend environment variables
# Set VITE_USE_FAKES to 'true' to use local fake data (default).
# Set to 'false' to make the frontend call the backend API at /api/clients.
VITE_USE_FAKES=true

# Optional: override API base URL used by axios boot (src/boot/axios.js)
VITE_API_BASE_URL=http://localhost:3333  # the composable will call /api/clients on this host
